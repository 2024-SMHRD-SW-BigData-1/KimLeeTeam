package Model;

public class StudentDTO {

	// 학생 한 명에 대한 정보가 하나의 자료형으로 관리될 수 있도록
	// 해주는 클래스
	
	private String name;
	private int age;
	private String gender;
	private String cls;
	public StudentDTO(String name, int age, String gender, String cls) {
		super();
		this.name = name;
		this.age = age;
		this.gender = gender;
		this.cls = cls;
	}
	public StudentDTO(String name,String cls) {
		this.name = name;
		this.cls = cls;
	}
	
	
	
	
	
	//생성자
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getGender() {
		return gender;
	}
	public void setGender(String gender) {
		this.gender = gender;
	}
	public String getCls() {
		return cls;
	}
	public void setCls(String cls) {
		this.cls = cls;
	}
	
	
	//getter,setter
	
	
	
}
