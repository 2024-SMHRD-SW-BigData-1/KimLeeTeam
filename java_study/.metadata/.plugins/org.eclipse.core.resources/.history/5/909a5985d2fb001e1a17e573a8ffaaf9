package Model;

public class DTO {


	private String U_ID;
	private String U_PW;
	private String U_NM;
	private int U_PH;


	
	// 로그인할 때 사용하는 생성자
	public DTO(String u_ID, String u_PW) {
		super();
		U_ID = u_ID;
		U_PW = u_PW;
	}

	// 회원가입할 때 사용하는 생성자
	public DTO(String u_ID, String u_PW, String u_NM, int u_PH) {
		super();
		U_ID = u_ID;
		U_PW = u_PW;
		U_NM = u_NM;
		U_PH = u_PH;
	}

	// getter, setter
	public String getU_NM() {
		return U_NM;
	}

	public void setU_NM(String u_NM) {
		this.U_NM = u_NM;
	}

	public String getU_ID() {
		return U_ID;
	}

	public void setU_ID(String u_ID) {
		this.U_ID = u_ID;
	}

	public String getU_PW() {
		return U_PW;
	}

	public void setU_PW(String u_PW) {
		this.U_PW = u_PW;
	}

	public int getU_PH() {
		return U_PH;
	}

	public void setU_PH(int u_PH) {
		this.U_PH = u_PH;
	}
	

}
